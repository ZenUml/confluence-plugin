{
  "key": "com.zenuml.confluence-addon",
  "name": "ZenUML Sequence Diagram",
  "description": "ZenUML Sequence Diagram add-on",
  "vendor": {
    "name": "ZenUML",
    "url": "https://www.zenuml.com/"
  },
  "baseUrl": "https://will.be.replaced.com",
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "enableLicensing": true,
  "links": {
    "self": "/atlassian-connect.json",
    "homepage": "https://www.zenuml.com"
  },
  "authentication": {
    "type": "none"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "zenuml-sequence-macro",
        "name": {
          "value": "ZenUML Sequence Diagram"
        },
        "renderModes": {
          "default": {
            "url": "/attachment?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}"
          }
        },
        "url": "/view.html?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}",
        "description": {
          "value": "Create a ZenUML Sequence Diagram"
        },
        "outputType": "block",
        "bodyType": "plain-text",
        "editor": {
          "url": "/edit.html?macroId={macro.id}&uuid={uuid}",
          "editTitle": {
            "value": "Edit Sequence Diagram"
          },
          "insertTitle": {
            "value": "Create Sequence Diagram"
          },
          "width": "1240px",
          "height": "500px"
        },
        "icon": {
          "width": 16,
          "height": 16,
          "url": "image/zenuml_logo.png"
        },
        "documentation": {
          "url": "https://www.zenuml.com/ZenUML_Sequence_Diagram_addon_help.html"
        },
        "categories": [
          "visuals"
        ],
        "featured": true
      }
    ]
  },
  "apiMigrations": {
    "gdpr": true
  }
}
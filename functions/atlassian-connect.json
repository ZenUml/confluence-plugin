{
  "key": "com.zenuml.confluence-addon",
  "name": "ZenUML Sequence Diagram",
  "description": "ZenUML Sequence Diagram add-on",
  "vendor": {
    "name": "ZenUML",
    "url": "https://www.zenuml.com/"
  },
  "baseUrl": "https://will.be.replaced.com",
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "enableLicensing": true,
  "links": {
    "self": "/atlassian-connect.json",
    "homepage": "https://www.zenuml.com"
  },
  "authentication": {
    "type": "none"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "zenuml-sequence-macro",
        "name": {
          "value": "ZenUML Sequence Diagram"
        },
        "renderModes": {
          "default": {
            "url": "/attachment?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}"
          }
        },
        "url": "/view.html?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}",
        "description": {
          "value": "Create a ZenUML Sequence Diagram"
        },
        "outputType": "block",
        "bodyType": "plain-text",
        "editor": {
          "url": "/sequence-launch-full-screen-editor.html?update=20210326",
          "editTitle": {
            "value": "Edit Sequence Diagram"
          },
          "insertTitle": {
            "value": "Create Sequence Diagram"
          },
          "width": "1240px",
          "height": "500px"
        },
        "icon": {
          "width": 16,
          "height": 16,
          "url": "image/zenuml_logo.png"
        },
        "documentation": {
          "url": "https://zenuml.atlassian.net/wiki/spaces/Doc/overview"
        },
        "categories": [
          "visuals"
        ],
        "featured": true
      },
      {
        "key": "zenuml-graph-macro",
        "name": {
          "value": "ZenUML Graph"
        },
        "renderModes": {
          "default": {
            "url": "/attachment?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}"
          }
        },
        "url": "/drawio/viewer.html?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}",
        "description": {
          "value": "Create a ZenUML Graph"
        },
        "outputType": "block",
        "bodyType": "none",
        "editor": {
          "url": "/drawio/initEditor.html",
          "editTitle": {
            "value": "Edit ZenUML Graph"
          },
          "insertTitle": {
            "value": "Create ZenUML Graph"
          },
          "width": "100%",
          "height": "100%"
        },
        "icon": {
          "width": 16,
          "height": 16,
          "url": "image/zenuml_logo.png"
        },
        "documentation": {
          "url": "https://zenuml.atlassian.net/wiki/spaces/Doc/overview"
        },
        "categories": [
          "visuals"
        ],
        "featured": true
      }
    ],
    "generalPages": [
      {
        "url": "/drawio/editor.html",
        "name": {
          "value": "drawio page"
        },
        "key": "editor-dialog-module-key"
      },
      {
        "url": "/sequence-editor.html?update=03260932",
        "name": {
          "value": "sequence page"
        },
        "key": "sequence-editor-dialog-module-key"
      }
    ]
  },
  "apiMigrations": {
    "gdpr": true
  }
}